{
    "cSpell.customDictionaries": {
        "custom": true,
        "internal-terms": true,
        "project-words": {
            "addWords": true,
            "description": "Words that Max uses a lot.",
            "name": "Max's Dictionary",
            "path": "$HOME/maxludden/.config/.cspell/maxDictionary.txt"
        },
        "python": true,
        "custom-dictionary-user": {
            "name": "custom-dictionary-user",
            "path": "$HOME/maxludden/.config/.cspell/maxDictionary.txt",
            "addWords": true,
            "scope": "user"
        }
    },
    "cSpell.userWords": [
        "colour",
        "configalias",
        "craigslist",
        "greylock",
        "maxbook",
        "maxludden",
        "pkgs",
        "shellenv",
        "signin",
        "supergene",
        "taila",
        "TODOC",
        "venvs",
        "webcolors",
        "WORKON"
    ],
    "cSpell.ignoreWords": [
        "ARGB",
        "Cfmq",
        "Couldn",
        "Didn",
        "Doesn",
        "EDWKN",
        "HEXA",
        "Hlwa",
        "Klsz",
        "Ludden",
        "Meslo",
        "Okqop",
        "Scrollback",
        "Soupify",
        "Sysnative",
        "XQAA",
        "ZQAC",
        "bpaste",
        "cssu",
        "docsmsft",
        "envops",
        "fordoc",
        "fxdioengyr",
        "jvalue",
        "pyenv",
        "pylint",
        "rmvirtualenv",
        "smms",
        "stepsize",
        "terragrunt",
        "vcmc",
        "windir",
        "ycclksmrfyglwe",
        "ygolq"
    ],
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": "${env:windir}\\System32\\cmd.exe",
            "args": [],
            "icon": "terminal-cmd"
        },
        "Ubuntu": {
            "path": "C:\\WINDOWS\\System32\\wsl.exe",
            "args": [
                "-d",
                "Ubuntu"
            ]
        }
    },
    "ruff.format.preview": true,
    "1password.debug.enabled": true,
    "settingsSync.ignoredSettings": [
        "-python.interpreter.infoVisibility"
    ],
    "files.autoSave": "afterDelay",
    "files.associations": {
        "*.jinja": "jinja-py"
    },
    "security.workspace.trust.enabled": true,
    "window.autoDetectColorScheme": true,
    "workbench.preferredHighContrastLightColorTheme": "Default Light Modern",
    "terminal.integrated.defaultProfile.windows": "Ubuntu",
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "cSpell.enableFiletypes": [
        "csv",
        "dotenv",
        "jinja-css",
        "jinja-html",
        "jinja-json",
        "shellscript",
        "ssh_config",
        "svg",
        "toml"
    ]
}
