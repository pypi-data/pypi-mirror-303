from app import db
from json import dumps

class User(db.Model):
    
    __tablename__ = 'users'

    id = db.Column(db.Integer, primary_key=True)
    

    def __repr__(self):
        return "User(id=%s)" % (self.id)
        
    def as_json(self):
        return dumps(self.as_dict(), ensure_ascii=False).encode('utf8')

    def as_dict(self):
    	raise NotImplementedError

